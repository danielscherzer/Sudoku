<Application x:Class="WpfSudoku.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 StartupUri="View/MainWindow.xaml">
    <Application.Resources>
		<SolidColorBrush x:Key="backgroundBrush" Color="White"/>
		<SolidColorBrush x:Key="borderBrush" Color="Black"/>
		<SolidColorBrush x:Key="activeBrush" Color="Yellow"/>
		<SolidColorBrush x:Key="invalidBrush" Color="Red"/>
		<SolidColorBrush x:Key="influencedBrush" Color="CornflowerBlue"/>
		<SolidColorBrush x:Key="selectionBrush" Color="DodgerBlue"/>
	</Application.Resources>
</Application>
